{
	"name": "sveltestarter",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "bun --bun vite build",
		"dev": "bun --bun vite dev",
		"preview": "bun --bun vite preview",
		"start": "bun --bun ./build/index.js",
		"check": "bun --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "bun --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "bun --bun eslint . --fix",
		"lint:css": "bun --bun stylelint .",
		"lint:ci": "bun --bun eslint .",
		"test": "bun --bun bun test:integration && bun test:unit",
		"test:integration": "bun --bun playwright test",
		"test:unit": "bun --bun vitest"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.15",
		"@neoconfetti/svelte": "^2.2.1",
		"@playwright/test": "^1.47.0",
		"@stylistic/eslint-plugin": "^2.8.0",
		"@sveltejs/kit": "^2.5.26",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.7",
		"@types/bun": "^1.1.10",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"clsx": "^2.1.1",
		"eslint": "^9.10.0",
		"eslint-plugin-svelte": "^2.43.0",
		"globals": "^15.9.0",
		"postcss": "^8.4.45",
		"stylelint": "^16.9.0",
		"stylelint-config-standard-scss": "^13.1.0",
		"svelte": "^5.0.0-next.244",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.0.1",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.5.0",
		"vite": "^5.4.3",
		"vitest": "^2.0.5"
	},
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@prisma/client": "^5.21.1",
		"axios": "^1.7.7",
		"bits-ui": "^0.21.16",
		"gsap": "^3.12.5",
		"lucia": "^3.2.2",
		"oslo": "^1.2.1",
		"prisma": "^5.21.1",
		"svelte-french-toast": "^1.2.0"
	}
}
