{
	"name": "sveltestarter",
	"version": "0.0.1",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "bun --bun vite build",
		"dev": "bun --bun vite dev",
		"preview": "bun --bun vite preview",
		"start": "bun --bun ./build/index.js",
		"check": "bun --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "bun --bun svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "bun --bun eslint . --fix",
		"lint:css": "bun --bun stylelint .",
		"lint:ci": "bun --bun eslint .",
		"test": "bun --bun bun test:integration && bun test:unit",
		"test:integration": "bun --bun playwright test",
		"test:unit": "bun --bun vitest"
	},
	"devDependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.6.0",
		"@fortawesome/free-regular-svg-icons": "^6.6.0",
		"@fortawesome/svelte-fontawesome": "^0.2.2",
		"@playwright/test": "^1.48.2",
		"@stylistic/eslint-plugin": "^2.10.1",
		"@sveltejs/kit": "^2.7.4",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bootstrap": "^5.2.10",
		"@types/bun": "^1.1.13",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"clsx": "^2.1.1",
		"eslint": "^9.14.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"postcss": "^8.4.47",
		"sass-embedded": "^1.80.6",
		"stylelint": "^16.10.0",
		"stylelint-config-standard-scss": "^13.1.0",
		"svelte": "^5.1.9",
		"svelte-adapter-bun": "^0.5.2",
		"svelte-check": "^4.0.5",
		"svelte-meta-tags": "^4.0.4",
		"svelte-typewriter": "^3.2.3",
		"svgo": "^3.3.2",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.14",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.2",
		"vite": "^5.4.10",
		"vite-plugin-image-optimizer": "^1.1.8",
		"vitest": "^2.1.4"
	},
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.6.0",
		"@popperjs/core": "^2.11.8",
		"axios": "^1.7.7",
		"bits-ui": "^0.21.16",
		"bootstrap": "^5.3.3",
		"jquery": "^3.7.1",
		"node-appwrite": "^14.1.0",
		"svelte-french-toast": "^1.2.0"
	}
}
